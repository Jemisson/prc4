<p id="notice"><%= notice %></p>

<h1>Atendimentos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Origem</th>
      <th>Status</th>
      <th>Responsavel</th>
      <th>Telefone</th>
      <th>Email</th>
      <th>Assunto</th>
      <th>Canal</th>
      <th>Notas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.name %></td>
        <td><%= attendance.lastname %></td>
        <td><%= attendance.origin %></td>
        <td><%= attendance.status %></td>
        <td><%= attendance.responsible %></td>
        <td><%= attendance.telephone %></td>
        <td><%= attendance.email %></td>
        <td><%= attendance.subject %></td>
        <td><%= attendance.channel %></td>
        <td><%= attendance.notes %></td>
        <td><%= link_to 'Show', attendance %></td>
        <td><%= link_to 'Edit', edit_attendance_path(attendance) %></td>
        <td><%= link_to 'Destroy', attendance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Atendimento', new_attendance_path %>
