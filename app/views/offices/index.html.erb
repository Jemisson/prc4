<p id="notice"><%= notice %></p>

<h1>Escritorios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Oab</th>
      <th>Cnpj</th>
      <th>Tipo</th>
      <th>Fundacao</th>
      <th>Endereco</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Cep</th>
      <th>Site</th>
      <th>Telefone</th>
      <th>Banco</th>
      <th>Agencia</th>
      <th>Conta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @escritorios.each do |escritorio| %>
      <tr>
        <td><%= escritorio.name %></td>
        <td><%= escritorio.oab %></td>
        <td><%= escritorio.cnpj_number %></td>
        <td><%= escritorio.type %></td>
        <td><%= escritorio.foundation %></td>
        <td><%= escritorio.adress %></td>
        <td><%= escritorio.city %></td>
        <td><%= escritorio.state %></td>
        <td><%= escritorio.zip %></td>
        <td><%= escritorio.site %></td>
        <td><%= escritorio.telephone %></td>
        <td><%= escritorio.bank %></td>
        <td><%= escritorio.agency %></td>
        <td><%= escritorio.account %></td>

        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Escritorio', new_office_path %>
