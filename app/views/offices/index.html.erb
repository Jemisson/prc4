<p id="notice"><%= notice %></p>

<h1>Escritorios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Oab</th>
      <th>Cnpj</th>
      <th>Tipo</th>
      <th>Fundacao</th>
      <th>Endereco</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Cep</th>
      <th>Site</th>
      <th>Telefone</th>
      <th>Banco</th>
      <th>Agencia</th>
      <th>Conta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% Office.all.each do |escritoriox| %>
      <tr>
        <td><%= escritoriox.name %></td>
        <td><%= escritoriox.oab %></td>
        <td><%= escritoriox.cnpj_number %></td>
        <td><%= escritoriox.society %></td>
        <td><%= escritoriox.foundation %></td>
        <td><%= escritoriox.adress %></td>
        <td><%= escritoriox.city %></td>
        <td><%= escritoriox.state %></td>
        <td><%= escritoriox.zip %></td>
        <td><%= escritoriox.site %></td>
        <td><%= escritoriox.telephone %></td>
        <td><%= escritoriox.bank %></td>
        <td><%= escritoriox.agency %></td>
        <td><%= escritoriox.account %></td>
        <td><%= link_to 'Show', escritoriox %></td>
        <td><%= link_to 'Edit', edit_office_path(escritoriox) %></td>
        <td><%= link_to 'Destroy', escritoriox, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Escritorio', new_office_path %>
