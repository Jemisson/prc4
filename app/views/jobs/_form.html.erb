<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

     <%= f.input :descricao %>
     <%= f.input :prazo %>
     <%= f.input :responsavel %>
     <%= f.input :status %>



  <%= f.button :submit, "Criar Job", class: "btn btn-primary" %>

<% end %>


