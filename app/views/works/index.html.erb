<p id="notice"><%= notice %></p>

<h1>Works</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Materia</th>
      <th>Acao</th>
      <th>Numero</th>
      <th>Honorariosp</th>
      <th>Honorariosp exfield</th>
      <th>Honorariosf</th>
      <th>Honorariosf exfield</th>
      <th>Honorarios trab x exito</th>
      <th>Honorarios parcelamento</th>
      <th>Escritorio</th>
      <th>Poderes</th>
      <th>Indicacao</th>
      <th>Indicacao comissao</th>
      <th>Pasta</th>
      <th>Atendimento inicial</th>
      <th>Advogado responsavel</th>
      <th>Advogado procuracao</th>
      <th>Estagiarios procuracao</th>
      <th>Paralegais procuracao</th>
      <th>Advogado parceiro</th>
      <th>Notas</th>
      <th>Checklist</th>
      <th>Checklist documentos</th>
      <th>Documentos pendentes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @works.each do |work| %>
      <tr>
        <td><%= work.tipo %></td>
        <td><%= work.materia %></td>
        <td><%= work.acao %></td>
        <td><%= work.numero %></td>
        <td><%= work.honorariosp %></td>
        <td><%= work.honorariosp_exfield %></td>
        <td><%= work.honorariosf %></td>
        <td><%= work.honorariosf_exfield %></td>
        <td><%= work.honorarios_trab_x_exito %></td>
        <td><%= work.honorarios_parcelamento %></td>
        <td><%= work.escritorio %></td>
        <td><%= work.poderes %></td>
        <td><%= work.indicacao %></td>
        <td><%= work.indicacao_comissao %></td>
        <td><%= work.pasta %></td>
        <td><%= work.atendimento_inicial %></td>
        <td><%= work.advogado_responsavel %></td>
        <td><%= work.advogado_procuracao %></td>
        <td><%= work.estagiarios_procuracao %></td>
        <td><%= work.paralegais_procuracao %></td>
        <td><%= work.advogado_parceiro %></td>
        <td><%= work.notas %></td>
        <td><%= work.checklist %></td>
        <td><%= work.checklist_documentos %></td>
        <td><%= work.documentos_pendentes %></td>
        <td><%= link_to 'Show', work %></td>
        <td><%= link_to 'Edit', edit_work_path(work) %></td>
        <td><%= link_to 'Destroy', work, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work', new_work_path %>
