<p id="notice"><%= notice %></p>

<h1>Atendimentos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Origem</th>
      <th>Status</th>
      <th>Responsavel</th>
      <th>Telefone</th>
      <th>Email</th>
      <th>Assunto</th>
      <th>Canal</th>
      <th>Notas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atendimentos.each do |atendimento| %>
      <tr>
        <td><%= atendimento.nome %></td>
        <td><%= atendimento.sobrenome %></td>
        <td><%= atendimento.origem %></td>
        <td><%= atendimento.status %></td>
        <td><%= atendimento.responsavel %></td>
        <td><%= atendimento.telefone %></td>
        <td><%= atendimento.email %></td>
        <td><%= atendimento.assunto %></td>
        <td><%= atendimento.canal %></td>
        <td><%= atendimento.notas %></td>
        <td><%= link_to 'Show', atendimento %></td>
        <td><%= link_to 'Edit', edit_atendimento_path(atendimento) %></td>
        <td><%= link_to 'Destroy', atendimento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atendimento', new_atendimento_path %>
